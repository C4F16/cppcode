Q1
#include <iostream>
using namespace std;
int main() {
    for (char ch1 = 'A'; ch1 <= 'Z'; ch1++) {
        for (char ch2 = 'A'; ch2 <= 'Z'; ch2++) {
            cout << ch1 << ch2 << endl;
        }
    }
    return 0;
}
-----------------------
Q2
#include <iostream>
using namespace std;
int main() {
    for (int i = 10; i >= 1; i--) {
        for (int j = 1; j <= i; j++)
        {
            cout << j << " ";

        }
        cout <<endl;
    }
    return 0;
}
