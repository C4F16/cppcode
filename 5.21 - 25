#include<iostream>
using namespace std;
int main()
{
	int age;
	cout << "entr ur age : ";
	cin >> age;
	if (age >= 18 && age <= 45) {
		cout << "Valid Age";
	}
	else
	{
		cout << "Tnualid Age";
	}
	return 0;

 }

