Q 46 
#include<iostream>
using namespace std;
int main() {
    for (char i = 'A'; i <= 'Z'; i++)
        cout << i<<endl;
        return 0;
}
-------------------------------------------
#include<iostream>                            
#include<cmath>
using namespace std;
void PrintLaters() {
	for (char i = 'A'; i <= 'Z'; i++)
{
		cout << i << endl;
}
}

int main() {
	PrintLaters();
	return 0;
}
