Q 49 
#include <iostream>
using namespace std;

int main() {
    int pin;
    int correctPin = 1234;
     int balance = 7500;

    cout << "Enter your ATM PIN: ";
    cin >> pin;

    if (pin == correctPin) {
        cout << "Your Balance is: " << balance << endl;
    } else {
        cout << "Wrong PIN" << endl;
    }

    return 0;
}


