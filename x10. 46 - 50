Q 49 
#include <iostream>
using namespace std;

int main() {
    int pin;
    int correctPin = 1234;
     int balance = 7500;

    cout << "Enter your ATM PIN: ";
    cin >> pin;

    if (pin == correctPin) {
        cout << "Your Balance is: " << balance << endl;
    } else {
        cout << "Wrong PIN" << endl;
    }

    return 0;
}
----------------------------------
#include<iostream>
#include<cmath>
using namespace std;
string ReadNumber( ) {
	string PIN;
	
		cout << "Enter PIN" << endl;
		cin >> PIN;
		return PIN;


}
bool Login() {
	string	PINCOD;
	do
	{
		PINCOD = ReadNumber();
		if (PINCOD == "1234") {
			return 1;
		}
		else
		{
			cout<<"error\n";
		}
	} while (PINCOD != "1234");
}



int main() {
	if (Login()) {
		cout << "ur account balnce is " << 7500<<endl;
	}
	
	return 0;
}
----------------------------------
Q 50 
#include<iostream>
#include<cmath>
using namespace std;
string ReadNumber( ) {
	string PIN;
	
		cout <<"Enter PIN" << endl;
		cin >> PIN;
		return PIN;


}
bool Login() {
	string	PINCOD;
	int i = 3;
	do
	{
		i--;
		PINCOD = ReadNumber();
		if (PINCOD == "1234") {
			return 1;
		}
		else
		{
			cout<<endl<<"error "<<" "<<"Weong PIN , You Have "<<i<<"More Times"<<endl;
		}
	} while (i>=1&&PINCOD != "1234");
}



int main() {
	if (Login()) {
		cout << "ur account balnce is " << 7500<<endl;
	}
	else
	{
		cout <<endl<< "Your card blocked call bank for helpe";
		
	}
	
	return 0;
}


