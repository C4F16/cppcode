//Q1

#include <iostream>
using namespace std;
void printurname(string name) {
	cout << "enter ur name : "<<name;

}
int main()
{
	printurname("ali");
	return 0;

}


#include <iostream>
using namespace std;

void printName() {
    cout << "Entr ur name" << endl;
}

int main() {
    printName();
    return 0;
}









//Q2


#include <iostream> 
#include <string>
using namespace std;
void printName() { 
    string name; 
    cout << "Entr ur name" << endl;
    getline(cin, name);
    cout << "ur name is : " << name;
}

int main() {
    printName();
    return 0;
}

































