Q12
#include <iostream>
using namespace std;
void readnumber(int& a, int& b) {
    cout << "enter number1 \n";
    cin >> a;
    cout << "enter number2 \n";
    cin >> b;
}
int MaxOf2Numbers(int a, int b) {
    if (a > b)
        return a;
    else
    {
        return b;
    }
}
void PrintMaxOf2Numbers(int Max) {
    cout << "The Maximum Number is : " << Max<<endl;

}
int main() {
    int a, b;
    readnumber(a, b);
    PrintMaxOf2Numbers(MaxOf2Numbers(a, b));
    return 0;
}
