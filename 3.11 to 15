Q14
#include <iostream>
using namespace std;
void readnumber(int& a, int& b) {
    cout << "enter number 1 \n";
    cin >> a;
    cout << "enter number 2 \n";
    cin >> b;
}
void SwapNumber(int &a , int &b) {
    int c;
    c = a;
    a = b;
    b = c;

}
void PrintSwapResult(int a , int b) {
    cout <<endl <<"Number 1 " << a<<endl;
    cout << "Number 2 " << b<<endl;

}
int main() {
    int a, b;
    readnumber(a, b);
    PrintSwapResult(a,b);
    SwapNumber(a, b);
    PrintSwapResult(a, b);
    return 0;
}
