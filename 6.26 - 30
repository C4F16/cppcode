Q27
#include<iostream>
using namespace std;
int main()
{
	int num;
	cout << "entr number : ";
	cin >> num;
	for (int i = num; i >= 1; i--) {
		cout << i<<endl;
	}
	return 0;

 }
-------------------------------------------
#include<iostream>
using namespace std;
int ReadNumber() {
	int a;
	cout << "enter Number " << endl;
	cin >> a;
	return a;
}

void PrintNumbersFromTo(int a) {
	for (int i = a;i >=1;i--) {
		cout << i << endl;
	}
}
int main() {



	PrintNumbersFromTo(ReadNumber());


	return 0;

}
