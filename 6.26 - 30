Q29 
#include <iostream>
using namespace std;

int main() {
    int num, sum = 0;
    cout << "Enter num";
    cin >> num;
    for (int i = 1; i <= num; i++) 
        if (i % 2 == 0) 
            sum += i;
        
    
    cout << "\nthe sum of even num for " << num << ":"<<sum;
    return 0;



















