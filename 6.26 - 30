Q28 
#include <iostream>
using namespace std;
int main() 
{
    int num, sum = 0;
    cout << "Enter number ";
    cin >> num;

    for (int i = 1; i <=num; i+=2) {

            sum += i;
    }

    cout << "sum = "<<sum;
    return 0;
}
------------------------------------------
#include<iostream>
using namespace std;
enum enEvenOrOdd{odd=1 , Even=2};
int ReadNumber() {
	int num;
	cout << "enter Number " << endl;
	cin >> num;

	return num;
}
enEvenOrOdd CheckEvenOrOdd(int num) {
	if (num % 2 != 0)
		return enEvenOrOdd::odd;
	else
		return enEvenOrOdd::Even;

}
int PrintSumOddNumbers(int a) {
	cout << "The Sum Of Odd Numbers : ";
	int sum = 0;
	for (int i = 1; i <= a; i++)
	{
		if (CheckEvenOrOdd(i) == enEvenOrOdd::odd)
		{
			sum += i;
		}
	}
	return sum;
}

int main() {
	int a =ReadNumber();
	cout<<PrintSumOddNumbers(a);

	return 0;

}
