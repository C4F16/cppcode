Q29 
#include <iostream>
using namespace std;

int main() {
    int num, sum = 0;
    cout << "Enter num";
    cin >> num;
    for (int i = 1; i <= num; i++) 
        if (i % 2 == 0) 
            sum += i;
        
    
    cout << "\nthe sum of even num for " << num << ":"<<sum;
    return 0;
------------------------------------------------------
#include<iostream>
using namespace std;
enum enEvenOrOdd{odd=1 , Even=2};
int ReadNumber() {
	int num;
	cout << "enter Number " << endl;
	cin >> num;

	return num;
}
enEvenOrOdd CheckEvenOrOdd(int num) {
	if (num % 2 == 0)
		return enEvenOrOdd::Even;
	else
		return enEvenOrOdd::odd;

}
int PrintSumEvenNumbers(int a) {
	cout << "The Sum Of Even Numbers : ";
	int sum = 0;
	for (int i = 1; i <= a; i++)
	{
		if (CheckEvenOrOdd(i) == enEvenOrOdd::Even)
		{
			sum += i;
		}
	}
	return sum;
}

int main() {
	int a =ReadNumber();
	cout<<PrintSumEvenNumbers(a);

	return 0;

}
