Q 30 
#include <iostream>
using namespace std;

int main() {
    int N;
    cout << "Enter a positive integer: ";
    cin >> N;

    long long factorial = 1;
    for (int i = 1; i <= N; ++i) {
        factorial *= i;
    }

    cout << "Factorial of " << N << " = " << factorial << endl;
    return 0;
}
---------------------------------------------------------------------
#include<iostream>
using namespace std;
int ReadNumber() {
	int num;
	cout << "enter a Poitive Number " << endl;
	cin >> num;

	return num;
}
int FactorialOfN(int num) {
	int Factorial=1;
	for (int i = num; i>=1; i--) 
	{
		Factorial*= i;

	}
	return Factorial;

}
void PrintFactorialOfN( int a) {
	cout << "The Factorial of N : "<<FactorialOfN(a);
}

int main() {
	int a =ReadNumber();
 PrintFactorialOfN(a);

	return 0;

}
