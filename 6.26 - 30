Q26
#include<iostream>
using namespace std;
int main()
{
	int num;
	cout << "entr number : ";
	cin >> num;
	for (int i = 1; i <= num; i++) {
		cout << i<<endl;
	}
	return 0;

 }
---------------------------------------
#include<iostream>
using namespace std;
int ReadNumber() {
	int a;
	cout << "enter Number " << endl;
	cin >> a;
	return a;
}

void PrintNumbersFromTo(int a) {
	for (int i = 1;i <= a;i++) {
		cout<<i<<endl;
	}
}
int main() {



	PrintNumbersFromTo(ReadNumber());

	
	return 0;

}

